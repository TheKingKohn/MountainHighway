{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "noImplicitAny": false
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules", 
    "dist", 
    "**/*.test.ts",
    "src/testing/**/*",
    "src/__tests__/**/*",
    "src/middleware/rateLimiting.broken.ts",
    "src/middleware/rateLimiting.complex.ts",
    "src/middleware/rateLimitingFixed.ts",
    "src/middleware/rateLimitingSafe.ts",
    "src/middleware/rateLimitingBasic.ts",
    "src/middleware/rateLimitingMinimal.ts"
  ]
}
